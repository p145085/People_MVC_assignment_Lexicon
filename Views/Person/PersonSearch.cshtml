@model List<Person>

<h1>Details page</h1>

<div class="row">
    <div class="col-1"><b>ID</b></div>
    <div class="col-1"><b>First Name</b></div>
    <div class="col-1"><b>Last Name</b></div>
    <div class="col-1"><b>Age</b></div>
    <div class="col-1"><b>City</b></div>
    <div class="col-1"><b>Phone</b></div>
    <div class="col-1"><b>Details</b></div>
</div>

@foreach (Person person in Model){
    <div class="row">
        <div class="col-1">@person.Id</div>
        <div class="col-1">@person.FirstName</div>
        <div class="col-1">@person.LastName</div>
        <div class="col-1">@person.Age</div>
        <div class="col-1">@person.City</div>
        <div class="col-1">@person.Phone</div>
    </div>
}


<a asp-action="ViewPeople">Back</a>